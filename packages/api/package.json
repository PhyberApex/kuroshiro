{
  "name": "kuroshiro-api",
  "version": "0.7.0",
  "private": true,
  "scripts": {
    "build": "tsup src/main.ts",
    "start:dev": "node scripts/copy_screens_from_ui.mjs && nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepare": "puppeteer browsers install chrome",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/config/typeorm.config.ts",
    "migration:create": "typeorm-ts-node-commonjs migration:create",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/config/typeorm.config.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/config/typeorm.config.ts",
    "migration:show": "typeorm-ts-node-commonjs migration:show -d src/config/typeorm.config.ts"
  },
  "dependencies": {
    "@nestjs/common": "catalog:",
    "@nestjs/config": "catalog:",
    "@nestjs/core": "catalog:",
    "@nestjs/platform-express": "catalog:",
    "@nestjs/serve-static": "catalog:",
    "@nestjs/typeorm": "catalog:",
    "class-transformer": "catalog:",
    "class-validator": "catalog:",
    "dotenv": "catalog:",
    "fs-extra": "catalog:",
    "pg": "catalog:",
    "puppeteer": "catalog:",
    "reflect-metadata": "catalog:",
    "rxjs": "catalog:",
    "typeorm": "catalog:"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:",
    "@nestjs/cli": "catalog:",
    "@nestjs/testing": "catalog:",
    "@swc/core": "catalog:",
    "@types/express": "catalog:",
    "@types/multer": "catalog:",
    "@types/node": "catalog:",
    "@types/supertest": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "eslint": "catalog:",
    "supertest": "catalog:",
    "ts-loader": "catalog:",
    "ts-node": "catalog:",
    "tsconfig-paths": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
