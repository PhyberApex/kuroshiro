{
  "name": "kuroshiro-api",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "build": "tsup src/main.ts",
    "start:dev": "node scripts/copy_screens_from_ui.mjs && nest start --watch --preserveWatchOutput",
