    "@nestjs/core": "11.1.9",
    "@nestjs/platform-express": "11.1.9",
    "@nestjs/serve-static": "5.0.4",
    "@nestjs/typeorm": "11.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.3",
    "dotenv": "17.2.3",
    "fs-extra": "11.3.2",
    "pg": "8.16.3",
    "puppeteer": "24.31.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2",
    "typeorm": "0.3.27"
  },
  "devDependencies": {
    "@antfu/eslint-config": "6.2.0",
    "@nestjs/cli": "11.0.14",
    "@nestjs/testing": "11.1.9",
    "@swc/core": "1.15.3",
    "@types/express": "5.0.6",
    "@types/multer": "2.0.0",
